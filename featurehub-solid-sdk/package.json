{
  "name": "featurehub-solid-sdk",
  "version": "1.0.1",
  "description": "The Solid SDK for FeatureHub",
  "private": false,
  "module": "dist/index.jsx",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ./**/*.{jsx,ts,tsx,md,json}",
