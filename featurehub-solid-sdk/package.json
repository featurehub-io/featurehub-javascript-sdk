    "solid-js": "^1.6.8"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-preset-solid": "^1.6.7",
    "eslint": "^8.31.0",
    "eslint-plugin-solid": "^0.9.2",
    "prettier": "2.8.2",
    "tsup": "6.5.0",
    "typescript": "^4.9.4"
