{
  "name": "featurehub-react-sdk",
  "version": "1.2.1",
  "description": "The React SDK for FeatureHub",
  "private": false,
  "author": "info@featurehub.io",
  "license": "MIT",
  "homepage": "https://featurehub.io",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rimraf node_modules dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "pnpm run build && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "featurehub-javascript-client-sdk": ">=1.3.0",
    "react": ">=18.2.0"
  },
  "devDependencies": {
    "@types/node": "catalog:node",
    "@types/react": "catalog:react19",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.37.5",
    "featurehub-javascript-client-sdk": "workspace:*",
    "featurehub-tsconfig": "workspace:*",
    "rimraf": "catalog:rimraf",
    "tsup": "catalog:tsup",
    "typescript": "catalog:typescript"
  },
  "keywords": [
    "feature-flag-api",
    "feature-flag-sdk",
    "feature-flag-react",
    "react-feature-flag",
    "feature-experimentation",
    "feature-flag",
    "flag",
    "toggle",
    "feature-toggle",
    "feature-toggle-api",
    "remote-configuration",
    "gradual-rollout",
    "ab-testing",
    "featurehub",
    "featurehub-react"
  ],
  "packageManager": "pnpm>=10.12"
}
