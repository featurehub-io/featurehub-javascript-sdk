{
  "name": "featurehub-react-sdk",
  "version": "1.2.1",
  "description": "The React SDK for FeatureHub",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
		"link": "npm link featurehub-javascript-client-sdk",
