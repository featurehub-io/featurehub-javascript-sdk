{
  "name": "featurehub-javascript-sdk",
  "version": "1.0.0",
  "description": "FeatureHub JavaScript SDKs",
  "author": "Anyway Labs",
  "homepage": "https://featurehub.io",
  "repository": "github:featurehub-io/featurehub-javascript-sdk",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build:packages && pnpm run build:examples",
    "build:packages": "pnpm --filter './packages/*' run build",
    "build:examples": "pnpm --filter './examples/*' run build",
    "build:core": "pnpm --filter './packages/core' run build",
    "build:js": "pnpm --filter './packages/js' run build",
    "build:node": "pnpm --filter './packages/node' run build",
    "build:react": "pnpm --filter './packages/react' run build",
    "build:solid": "pnpm --filter './packages/solid' run build",
    "clean": "pnpm -r run clean && rimraf node_modules",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "link:core": "pnpm --filter './packages/core' run link",
    "link:node": "pnpm --filter './packages/node' run link",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "pnpm run test:packages && pnpm run test:examples",
    "test:examples": "pnpm --filter './examples/*' run test",
    "test:packages": "pnpm --filter './packages/*' run test",
    "test:js": "pnpm --filter './packages/js' run test",
    "test:server": "pnpm --filter './examples/todo-server-tests' run test",
    "typecheck": "pnpm -r run typecheck"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "9.38.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-solid": "0.14.5",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "catalog:prettier",
    "rimraf": "catalog:rimraf",
    "typescript": "catalog:typescript",
    "typescript-eslint": "^8.46.2"
  }
}
