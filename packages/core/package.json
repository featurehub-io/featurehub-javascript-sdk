{
  "name": "featurehub-javascript-core-sdk",
  "version": "1.5.0",
  "description": "FeatureHub core SDK - not intended to be used directly",
  "author": "info@featurehub.io",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "lib/index.js",
  "repository": "github:featurehub-io/featurehub-javascript-sdk",
  "files": [
    "dist"
  ],
  "homepage": "https://featurehub.io",
  "keywords": [
    "feature-flag-api",
    "feature-flag-sdk",
    "feature-experimentation",
    "feature-flag",
    "flag",
    "toggle",
    "feature-toggle",
    "feature-toggle-api",
    "remote-configuration",
    "gradual-rollout",
    "ab-testing",
    "feature-flag-react",
    "featurehub"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rimraf node_modules dist",
    "tsc": "node ./node_modules/typescript/bin/tsc -p",
    "release": "cp ../../README.md . && pnpm run build && pnpm publish --no-git-checks --provenance --access public",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.208.0",
    "@types/jsdom": "^27.0.0",
    "@types/netmask": "^2.0.5",
    "@types/node": "catalog:node",
    "@types/semver-compare": "^1.0.3",
    "@vitest/coverage-istanbul": "catalog:vitest",
    "featurehub-tsconfig": "workspace:*",
    "jsdom": "^27.1.0",
    "rimraf": "catalog:rimraf",
    "@types/sinon": "catalog:sinon",
    "sinon": "catalog:sinon",
    "tsup": "catalog:tsup",
    "typescript": "catalog:typescript",
    "vite": "7.2.0",
    "vitest": "catalog:vitest"
  },
  "dependencies": {
    "murmurhash": "^2.0.1",
    "netmask": "^2.0.2",
    "semver-compare": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.18.3"
}
