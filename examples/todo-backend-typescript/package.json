{
  "name": "featurehub-node-example",
  "version": "2.1.0",
  "main": "index.js",
  "module": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "clean": "rimraf node_modules dist",
    "setup": "pnpm link ../../packages/js ../../packages/node",
    "compile": "pnpm run setup && pnpm run build",
    "start": "pnpm run build && node --trace-deprecation --trace-warnings dist/app.js",
    "run": "pnpm run build && node --trace-deprecation --trace-warnings dist/app.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "featurehub-javascript-node-sdk": "^1.3.2",
    "restify": "^9.1.0",
    "restify-cors-middleware2": "^2.2.0",
    "vite": "7.1.10"
  },
  "devDependencies": {
    "@types/node": "catalog:node",
    "@types/restify": "^8.5.6",
    "@types/restify-cors-middleware": "^1.0.1",
    "featurehub-tsconfig": "workspace:*",
    "rimraf": "catalog:rimraf",
    "ts-node": "10.9.1",
    "typescript": "catalog:typescript"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm>=10.12"
}
