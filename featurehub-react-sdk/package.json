    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "prettier": "^2.8.3",
    "tsup": "6.5.0",
    "typescript": "^4.9.4"
  },
  "author": "info@featurehub.io",
  "license": "MIT",
  "homepage": "https://featurehub.io",
  "keywords": [
    "feature-flag-api",
